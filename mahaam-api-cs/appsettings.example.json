{
  "app": {
    "api": {
      "name": "mahaam-api-cs",
      "version": "1.0",
      "envName": "local",
      "httpPort": 7023,
      "tokenSecretKey": "your-secret-key"
    },
    "logging": {
      "file": "/path/to/log/trace.log",
      "fileSizeLimit": 20971520,
      "fileCountLimit": 31,
      "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u3} {Message:lj}{NewLine}{Exception}"
    },
    "email": {
      "accountSid": "your-twilio-account-sid",
      "verificationServiceSid": "your-verification-service-sid",
      "authToken": "your-auth-token",
      "testEmails": ["email1@example.com", "email2@example.com", "email3@example.com"],
      "testSID": "your-test-sid",
      "testOTP": "your-test-otp"
    },
    "dbUrl": "Host=localhost;Port=5432;Username=postgres;Password=_your_password;Database=mahaam;Search Path=app;Pooling=true",
    "logReqEnabled": false
  }
}
